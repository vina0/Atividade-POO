public class Pessoa {

   private String nome;
   private int datanascimento;
   private int idade;

public Pessoa(String nome, int nascimento, int idade) {
  this.nome = nome;
  this.datanascimento = nascimento;
  this.idade = idade;
}
public String getnome(){
    return this.nome;
}
public int getnascimento(){
    return this.datanascimento;
}
public int getidade(){
    return this.idade;
}
}

